table.ui.celled.unstackable.striped.table
  thead.color-lighter
    tr
      th
        |date
      th
        |sat
      th
        |unsat
      th
        |timeout
      th
        |misc
      th
        |commit
  tbody
    - results.each do |result|
      tr
        td.collapsing
            |#{result.date}
        td.positive
          |#{result.sat}
        td.error
          |#{result.unsat}
        td.warning
          |#{result.timeout}
        td.active
          |#{result.misc}
        td.collapsing.right.aligned
          - if tool_url != nil && result.commit != "-"
            a href="javascript:goto_commit('#{tool_url}','#{result.commit}');"
              |#{result.commit}
          - else
            |-

